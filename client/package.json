{
  "name": "chores",
  "version": "0.1.0",
  "description": "Front-end for Chores app",
  "repository": "jochakovsky/chores",
  "main": "index.js",
  "scripts": {
    "test:lint": "eslint \"src/**/*.@(js|jsx)\"",
    "test": "npm run test:lint",
    "precompile": "rm -rf build && mkdir -p build",
    "compile": "npm run compile:js & npm run compile:html",
    "compile:html": "cp src/index.html build/index.html",
    "compile:js": "browserify -t [ babelify ] src/index.jsx -o build/bundle.js",
    "prestart": "npm run compile",
    "start": "http-server build -p 8080 -a 0.0.0.0"
  },
  "author": "Joshua Ochakovsky",
  "license": "GPL-3.0",
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "http-server": "^0.9.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "eslint": "^2.11.1",
    "eslint-plugin-react": "^5.1.1"
  }
}
